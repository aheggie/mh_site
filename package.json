{
  "name": "healing-through-generations-mainsite",
  "version": "0.0.9",
  "description": "Mental health website for racialised practitioners and clientele",
  "main": "src/static/index.html",
  "scripts": {
    "clear": "rm -rf dist",
    "winclear": "rmdir /s /q dist",
    "cleardev": "npm run clear && npm run dev",
    "clearbuild": "npm run clear && npm run build",
    "dev": "parcel src/static/index.html",
    "build": "parcel build src/static/index.html",
    "deploy:h": "firebase deploy --only hosting:htg-dev-main",
    "cdd:h": "npm run cleardev && npm run deploy:h",
    "cbd:h": "npm run clearbuild && npm run deploy:h"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aheggie/mh_site.git"
  },
  "author": "CURE Concordia (Jodie-Ann Muckler, Alexandra Heggie, Cassie Smith)",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/aheggie/mh_site/issues"
  },
  "homepage": "https://github.com/aheggie/mh_site#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "html-react-parser": "^0.13.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  }
}
